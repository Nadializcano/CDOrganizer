<h3>Here are all of the CDs under that Artist</h3>

<ol>
  @foreach (var cd in @Model["cds"])
  {
    <li><a href='/artists/@Model["artist"].GetId()/cds/@cd.GetId()'>@cd.GetDescription()</a></li>

  }
</ol>

<p><a href='/artists/@Model["artist"].GetId()/cds/new'>Add a CD</a></p>
<p><a href='/artists'>Return to Artist list</a></p>
<p><a href='/'>Return to Main Page</a></p>
